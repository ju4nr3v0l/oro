<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        {% block metas %}
            <meta name="viewport" content="width=device-width, initial-scale=1.0">
            <meta name="description" content="zircosTemplate">
            <meta name="author" content="Alejandro Vera Carrasquilla">
        {% endblock %}
        <title>{% block title %}Bienvenido!{% endblock %}</title>
        {% block stylesheets %}
            <!-- Custom box css -->
            <link href="{{ asset('zircosTemplate/plugins/custombox/css/custombox.min.css') }}" rel="stylesheet">
            <link href="{{ asset('zircosTemplate/assets/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('zircosTemplate/assets/css/core.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('zircosTemplate/assets/css/components.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('zircosTemplate/assets/css/icons.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('zircosTemplate/assets/css/pages.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('zircosTemplate/assets/css/menu.css') }}" rel="stylesheet" type="text/css" />
            <link href="{{ asset('zircosTemplate/assets/css/responsive.css') }}" rel="stylesheet" type="text/css" />
            <link rel="stylesheet" href="{{ asset('zircosTemplate/plugins/switchery/switchery.min.css') }}">
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('zircosTemplate/assets/images/favicon.ico') }}" />
        {% block javascripts %}
            <script src="{{ asset('zircosTemplate/assets/js/modernizr.min.js') }}"></script>


        {% endblock %}
    </head>
    <body {% block bodyAtt %}{% endblock %} >


        {% block body %}

            {% block content %}

            {% endblock %}

        {% endblock %}


        {% block javascriptsBottom %}
            <script>
                var resizefunc = [];
            </script>
            <!-- jQuery  -->



            <script src="{{ asset('zircosTemplate/assets/js/jquery.min.js') }}"></script>
            <script src="{{ asset('zircosTemplate/assets/js/bootstrap.min.js') }}"></script>
            <script src="{{ asset('zircosTemplate/assets/js/detect.js') }}"></script>
            <script src="{{ asset('zircosTemplate/assets/js/fastclick.js') }}"></script>
            <script src="{{ asset('zircosTemplate/assets/js/jquery.blockUI.js') }}"></script>
            <script src="{{ asset('zircosTemplate/assets/js/waves.js') }}"></script>
            <script src="{{ asset('zircosTemplate/assets/js/jquery.slimscroll.js') }}"></script>
            <script src="{{ asset('zircosTemplate/assets/js/jquery.scrollTo.min.js') }}"></script>
            <script src="{{ asset('zircosTemplate/plugins/switchery/switchery.min.js') }}"></script>
            <script src="{{ asset('zircosTemplate/plugins/bootstrap-inputmask/bootstrap-inputmask.min.js') }}" type="text/javascript"></script>
            <script type="text/javascript" src="{{ asset('zircosTemplate/plugins/parsleyjs/parsley.min.js') }}"></script>

            <!-- Modal-Effect -->
            <script src="{{ asset('zircosTemplate/plugins/custombox/js/custombox.min.js') }}"></script>
            <script src="{{ asset('zircosTemplate/plugins/custombox/js/legacy.min.js') }}"></script>

            <!-- App js -->
            <script src="{{ asset('zircosTemplate/assets/js/jquery.core.js') }}"></script>
            <script src="{{ asset('zircosTemplate/assets/js/jquery.app.js') }}"></script>

            {#<script src="{{ asset('https://code.jquery.com/jquery-2.2.4.js') }}" integrity="sha256-iT6Q9iMJYuQiMWNd9lDyBUStIq/8PuOW33aOqmvFpqI=" crossorigin="anonymous"></script>#}
            {#<script src="{{ asset('js/oro.js') }}"></script>#}
        {% endblock %}
    </body>
</html>
